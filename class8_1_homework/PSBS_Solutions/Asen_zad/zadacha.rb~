require "csv"
all= Array.new()
bukva=ARGV[1].to_s
string=String.new()
a=0
i=0
p bukva
 CSV.foreach(ARGV[0],"r") do |row|
string=row[0].to_s
a=string.length
 p string
begin
if string[i]==bukva
all<<[row[0],row[1],row[2]]
end
++i

end while ( i==a)
i=0
end

CSV.open("res.csv","w") do |csv| 
all.each do |l| 
csv << [l[0],l[1],l[2]]
end
end
